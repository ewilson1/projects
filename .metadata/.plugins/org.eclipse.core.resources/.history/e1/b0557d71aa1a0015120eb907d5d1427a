Ti.UI.setBackgroundColor("fff");

//create a window to hold the Navigation window
var mainWindow = Ti.UI.createWindow({
	title: "Eric Wilson's Weather",
	backgroundImage: "Weather-Background.jpg"	
});
	var navWindow = Ti.UI.iOS.createNavigationWindow({
		window: mainWindow
	});
		var view1 = Ti.UI.createView({
		  backgroundColor: 'white',
		  height: 100,
		  width: 300,
		  borderRadius: 20
		});
		
		var label1 = Ti.UI.createLabel({
			text: "Click to open up the Menu",
			textAlign: "center"
		});
		view1.add(label1);

//Now, require both .js files here on the bootstrap page, in backwards order...
//var pictures = require("Menu");

//add the label, open the main Navigaton window
navWindow.add(view1);
navWindow.open();
